{
    "version": 2,
    "builds": [
      {
        "src": "backend/app.py",
        "use": "@vercel/python",
        "config": {
          "includeFiles": ["backend/gradient_boost_model.pkl"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend/app.py"
      }
    ]
  }